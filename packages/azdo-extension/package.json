{
  "name": "@uipath-xaml-visualizer/azdo-extension",
  "version": "1.0.0",
  "description": "Azure DevOps拡張機能でUiPath XAMLファイルを視覚化",
  "main": "dist/main.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:test": "webpack --config webpack.test.config.js",
    "dev": "webpack --mode development --watch",
    "package": "tfx extension create --manifest-globs vss-extension.json",
    "publish": "tfx extension publish --manifest-globs vss-extension.json",
    "serve": "npx http-server -p 8080 -c-1"
  },
  "keywords": [
    "uipath",
    "xaml",
    "azure-devops",
    "visualizer"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.6",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "tfx-cli": "^0.17.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@uipath-xaml-visualizer/shared": "^1.0.0",
    "azure-devops-extension-api": "^4.234.0",
    "azure-devops-extension-sdk": "^4.0.2"
  }
}
